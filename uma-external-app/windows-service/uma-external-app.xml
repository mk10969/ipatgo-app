<configuration>
    <id>UMA-EXTERNAL-APP</id>
    <name>MyApp Service (powered by WinSW)</name>
    <description>This is jvlink service and ipatgo service</description>

    <executable>java</executable>
    <argument>-Xloggc:./log/gc.log</argument>
    <argument>-XX:+PrintGCDetails</argument>
    <argument>-XX:+PrintGCTimeStamps</argument>
    <argument>-jar</argument>
    <argument>Z:\uma-external-app.jar</argument>
    <argument>--spring.profiles.active=prd</argument>
    <argument>--ipatgo.authentication.INetId=XXXXXX</argument>
    <argument>--ipatgo.authentication.subscriberNo=XXXXXX</argument>
    <argument>--ipatgo.authentication.password=XXXXXX</argument>
    <argument>--ipatgo.authentication.parsNo=XXXXXX</argument>

    <logpath>%BASE%\log</logpath>

</configuration>